<h2>New book</h2>
  <%= form_with model:@book ,url:books_path do |f|%>
    <div class="form-group">
      Title</br>
      <%= f.text_field :title,class:"form-control"%>
    </div>
    <div class="form-group">
      Opinion</br>
      <%= f.text_area :body,class:"form-control"%>
    </div>
    <%= f.submit"Create Book",class:"btn btn-success"%>
    <% end %>
</div>